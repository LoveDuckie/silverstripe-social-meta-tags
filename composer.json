{
    "name": "loveduckie/silverstripe-social-meta-tags",
    "description": "A skeleton for SilverStripe modules.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "cms", "silverstripe-4", "social-meta-tags", "social-share"],
    "authors": [{
        "name": "Luc Shelton",
        "email": "lucshelton@gmail.com"
    }],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "http://github.com/LoveDuckie/silverstripe-social-meta-tags/issues"
    },
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LoveDuckie\\SilverStripe\\Skeleton\\": "src/",
            "LoveDuckie\\SilverStripe\\Skeleton\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
