{
    "name": "loveduckie/silverstripe-social-meta-tags",
    "description": "A Silverstripe module for injecting OpenGraph and Twitter meta tags for sharing and social previews.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "cms", "silverstripe-4", "social-meta-tags", "social-share", "silverstripe-5"],
    "authors": [
        {
            "name": "Luc Shelton",
            "email": "lucshelton@gmail.com",
            "homepage": "https://www.lucshelton.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "lucshelton@gmail.com",
        "issues": "http://github.com/LoveDuckie/silverstripe-social-meta-tags/issues"
    },
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0 | ^5.0",
        "silverstripe/admin": "^1.0 | ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LoveDuckie\\Silverstripe\\SocialMetaTags\\": "src/",
            "LoveDuckie\\Silverstripe\\SocialMetaTags\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
